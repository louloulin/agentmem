{
  "name": "@agentmem/client",
  "version": "6.0.0",
  "description": "Official JavaScript/TypeScript SDK for AgentMem - Enterprise-grade memory management for AI agents",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write src/**/*.{ts,js}",
    "format:check": "prettier --check src/**/*.{ts,js}",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "ai",
    "memory",
    "agent",
    "llm",
    "vector",
    "database",
    "embedding",
    "semantic",
    "search",
    "enterprise",
    "typescript",
    "javascript"
  ],
  "author": "AgentMem Team <support@agentmem.dev>",
  "license": "MIT",
  "homepage": "https://agentmem.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentmem/agentmem.git",
    "directory": "sdks/javascript"
  },
  "bugs": {
    "url": "https://github.com/agentmem/agentmem/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "axios-retry": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.6.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
